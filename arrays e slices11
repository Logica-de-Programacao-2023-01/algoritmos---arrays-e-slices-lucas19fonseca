package main

import "fmt"

func main() {

	array := [2][3]int{
		{1, 2, 3},
		{4, 5, 6},
	}

	var line, coluna int

	fmt.Print("Informe a line: ")
	fmt.Scanln(&line)
	fmt.Print("Informe a coluna: ")
	fmt.Scanln(&coluna)

	fmt.Printf("O valor armazenado na posição (%d,%d) é %d", line, coluna, array[line][coluna])
}
